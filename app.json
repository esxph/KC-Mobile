{
  "expo": {
    "name": "KCMobileApp",
    "slug": "KCMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kcmobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "KCMobileApp",
      "shortName": "KCApp",
      "startUrl": "/",
      "scope": "/",
      "display": "standalone",
      "backgroundColor": "#ffffff",
      "themeColor": "#ffffff",
      "lang": "en",
      "description": "KCMobile as a PWA",
      "meta": {
        "apple": {
          "statusBarStyle": "default"
        },
        "themeColor": "#ffffff"
      },
      "webManifest": {
        "icons": [
          {
            "src": "./assets/images/icon.png",
            "sizes": "192x192",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": "expo-router"
    }
  }
}